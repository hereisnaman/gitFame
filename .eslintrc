{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0,
    "react/style-prop-object": 0,
    "no-unused-vars": 1,
    "jsx-a11y/anchor-is-valid": 0
  }
}
